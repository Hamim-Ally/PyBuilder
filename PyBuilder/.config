build:
  script_name: src/build.py  # Path to your Python script
  icon: "src/.ico"  # Path to the icon file
  onefile: true  # Whether to create a single executable file
  distpath: "./dist"  # Output folder for the .exe file
  workpath: "./build"  # Working folder for PyInstaller
  specpath: "./"  # Folder for PyInstaller spec files
  additional_options: []  # Additional PyInstaller options if needed
